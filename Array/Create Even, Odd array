#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,arr[10]= {30,33,12,13,74,75,60,5,6,3};
    int earr[10],oarr[10];
    cout<<"Even numbers array:";
    for(i=0; i<10; i++)
    {
        if(arr[i]%2==0 )
        {
            earr[j]=arr[i];
            cout<<earr[j]<<" ";
        }
    }
    cout<<"\nOdd Numbers array:";
    for(i=0; i<10; i++)
    {
        if(arr[i]%2!=0 )
        {
            oarr[j]=arr[i];
            cout<<oarr[j]<<" ";
        }
    }
    return 0;
}
