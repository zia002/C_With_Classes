#include<iostream>
using namespace std;
int main()
{
    int i,arr[9]= {3,5,1,8,7,9,4,2,6};
    cout<<"Primary Array=";
    for(i=0; i<9; i++)
    {
        cout<<arr[i]<<" ";
    }
    int n,p;
    cout<<"\nWhich value do you want to insert?";
    cin>>n;
    cout<<"In which position from 0 to 8 do you want to insert?";
    cin>>p;
    arr[p]=n;
    cout<<"Update Array=";
    for(i=0; i<9; i++)
    {
        cout<<arr[i]<<" ";
    }
    return 0;
}
