#include<bits/stdc++.h>
using namespace std;
int main()
{
    int i,j,arr[3][3],count0=0,count=0;
    cout<<"Enter 3Ã—3 Matrix Value:\n";
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
        {
            cout<<"array["<<i<<"]["<<j<<"]=";
            cin>>arr[i][j];
        }
    }
    cout<<"Matrix:\n";
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
        {
            cout<<arr[i][j]<<" ";
        }
        cout<<"\n";
    }
    for(i=0; i<3; i++)
    {
        for(j=0; j<3; j++)
        {
            if(arr[i][j]==0)
            {
                count0=count0 + 1;
            }
            else
            {
            count=count+1;
            }
        }
    }
    if(count0>count)
    {
    cout<<"Yes! It Is A Sparse Matrix";
    }
    else
    cout<<"No! It Is Not A Sparse Matrix";
    return 0;
}
