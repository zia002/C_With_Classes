#include<iostream>
using namespace std;
int main()
{
    int i,arr[9]= {3,5,1,8,7,9,4,2,6};
    int max=arr[0];
    for(i=1; i<9; i++)
    {
        if(max<arr[i])
        {
            max=arr[i];
        }
    }
    cout<<"Max value:"<<max<<endl;
    int min=arr[0];
    for(i=1; i<9; i++)
    {
        if(min>arr[i])
        {
            min=arr[i];
        }
    }
    cout<<"min value:"<<min<<endl;
    return 0;
}
