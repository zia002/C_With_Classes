//Product of number digits
#include<iostream>
using namespace std;
int main()
{
    int n,tem,rem;
    int product=1;
    cout<<"Enter Your number:";
    cin>>n;
    tem=n;
    while(tem!=0)
    {
        rem=tem%10;
        tem=tem/10;
        product=product*rem;
    }
    cout<<"Product Of digits:"<<product;
    return 0;
}
