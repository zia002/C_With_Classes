//Hollow__Rectangle
#include<iostream>
using namespace std;
int main()
{
    int r,c,i,j;
    cout<<"Enter Row,Column Value:";
    cin>>r>>c;
    for(i=1; i<=r; i++)
    {
        for(j=1; j<=c; j++)
        {
            if(i==1 || i==r ||j==c || j==1)
            {
                cout<<"â˜…";
            }
            else
            {
                cout<<" ";
            }
        }
        cout<<"\n";
    }
    return 0;
}
